<%= form_for [:relax_admin, @model] do |f| %>
  <%#= render 'relax_admin/shared/debug', params: params %>

  <%= render 'relax_admin/shared/errors_data_new' %>

  <% params.each do |a| %>
    <%# <strong><%= @model_class.type_for_attribute(a).type.to_s </strong> %>
    <%= render 'relax_admin/fields/form_group', f: f, a: a %>
  <% end %>

  <%= render 'relax_admin/base/translatable_fields', f: f %>

  <%= render 'relax_admin/shared/new_form_buttons' %>
<% end %>

<div class="modal-belongs-to modal"></div>
